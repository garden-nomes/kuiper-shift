{
  "name": "nokia-3310-jam",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:assets": "pota-8 bundle-assets -w ./assets",
    "build": "tsc --noEmit && vite build",
    "build:assets": "pota-8 bundle-assets ./assets",
    "serve": "vite preview",
    "deploy": "npm run build -- --base=/tiny-asteroid-miner/ && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "devDependencies": {
    "push-dir": "^0.4.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50"
  },
  "dependencies": {
    "pota-8": "^0.2.3",
    "simplex-noise": "^2.4.0"
  }
}
